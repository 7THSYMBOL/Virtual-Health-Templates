    <add key="ida:ClientId" value="CLIENT_ID" />
    <!--Client Secret of the application registered in the Azure AD-->
    <add key="ida:ClientSecret" value="CLIENT_SECRET" />
    <!--O365 Tenant Domain-->
    <add key="ida:Domain" value="xxxx.onmicrosoft.com" />
    <!--O365 Tenant ID-->
    <add key="ida:TenantId" value="GUID_TENANT_ID" />
    <!--Native app client ID registered in Azure AD-->
    <add key="ida:NativeAppClientId" value="NATIVE_CLIENT_APP_ID" />
    <!--User name for the Native Client APP-->
    <add key="ida:UserName" value="xxx@xxxx.onmicrosoft.com" />
    <!--SharePoint site collection URL-->
    <add key="SharepointSite" value="https://xxxx.sharepoint.com/sites/xxxxxx" />
    <!--User name to access SharePoint resources-->
    <add key="SPOUserName" value="xxxx@xxxxx.onmicrosoft.com" />
    <!--Post logout URR-->
    <add key="ida:PostLogoutRedirectUri" value="https://xxxxxx.azurewebsites.net/" />
    <!--Meeting subject to create UCAP meetings-->
    <add key="ida:MeetingSubject" value="Appointment Details" />
    <!--Azure webapp URL where virtual health is hosted-->
    <add key="ida:HealthCarePortal" value="https://xxxxxx.azurewebsites.net/" />
    <!--Key vault base URL-->
    <add key="KeyVaultBaseUrl" value="https://xxxx.vault.azure.net" />
    <!--Client ID of the application registered in Azure AD to access Key Vault-->
    <add key="ClientId" value="CLIENT_ID_GUID" />
    <!--Thumbprint of the certificate to access Key Vaulty-->
    <add key="Thumbprint" value="CERTIFICATE_THUMBPRINT" />
    <!--Trusted API enpoint-->
    <add key="TrustedApi" value="HTTPS_URI_TRUSTED_APP" />
    <!--Application Insight Key-->
    <add key="iKey" value="APP_INSIGHTS_ID" />
    <!--Bot embed URI-->
    <add key="botUrlEmbed" value="https://webchat.botframework.com/embed/xxxxxx?s=xxxxxx" />
    <!--Lamna Web API URI-->
    <add key="MobileSiteUri" value="lamna://xxxx.yyyy.Lamna/" />
    <!--Demo User ID to create meeting for anonymous users-->
    <add key="DemoUserId" value="xxxxx@xxxxxxx.onmicrosoft.com" />
    <!--Email server to send email-->
    <add key="EmailServer" value="smtp.office365.com" />
    <!--Admin group to access the configuration page of the Virtual Health-->
    <add key="SharepointAdminGroup" value="SHAREPOINT_GROUP_NAME" />
  </appSettings>
